<% provide(:title, "Editar Linea de Credito") %>
<h1>Update your profile</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@credit_line) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>

      <%= f.label :name %>
      <%= f.text_field :name %>

      <%= f.label :annual_Rate %>
      <%= f.text_field :annualRate %>

      <%= f.submit "Guardar cambios", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>